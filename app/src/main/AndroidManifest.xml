<?xml version="1.0" encoding="utf-8"?>
<manifest
    xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ver2point0.android.blocspot">

    <application
        android:name="com.ver2point0.android.blocspot.BlocSpotApplication"
        android:allowBackup="true"
        android:label="@string/app_name"
        android:icon="@drawable/ic_launch_icon"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.ver2point0.android.blocspot.ui.activity.BlocSpotActivity">
            <intent-filter>

                <action android:name="android.intent.action.MAIN"/>

                <action android:name="android.intent.category.LAUNCHER"/>

            </intent-filter>
        </activity>
    </application>
</manifest>
